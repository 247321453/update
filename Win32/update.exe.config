<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
	</startup>
	<appSettings>
		<!-- access these values via the property:
            System.Configuration.ConfigurationManager.AppSettings[key]
        -->
		<add key="url" value="https://github.com/247321453/update/raw/master/" />
		<!-- game save path -->
		<add key="path" value="D:\ygopro" />
		<!-- use proxy -->
		<add key="useproxy" value="false" />
		<add key="proxy" value="127.0.0.1:8080" />
		<!-- if file exitis then no download -->
		<add key="ignore1" value="textrue/*" />
		<add key="ignore2" value="deck/*" />
	</appSettings>
</configuration>